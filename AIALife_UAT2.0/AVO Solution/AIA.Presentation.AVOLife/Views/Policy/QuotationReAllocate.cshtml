@model AIA.Life.Models.Policy.Policy
@using AIA.Presentation.Helpers
@{
    ViewBag.Title = "QuotationReAllocate";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .panel-default {
        /*border-color: #eee !important;*/
        margin-bottom: 5px;
    }

    .panel {
        background-color: #B8123E !important;
    }
    @@media screen and (min-width:320px) and (max-width:768px){
 .no-more-tables table, thead, tbody, th, td, tr
    {
       
        float:none;
       
    }
}
</style>

<div class="panel panel-default">
    <div class="panel-body" style="color: white;
    font-weight: 600;">New Business - Quotation - Quotation Re-Allocate</div>
</div>

<div class="" id="divQuotationReAllocateGrid" style="overflow-x:auto;">

</div>


<script type="text/javascript">

    $(document).ready(function () {
        $.get("../Policy/SearchQuotationReAllocateDetails", $("").serialize())
               .done(function (data) {
                   $("#divQuotationReAllocateGrid").html(data);
               }).
               fail(function () {
                   $("#divQuotationReAllocateGrid").html("No Records Found");
               });

    });
</script>

