    function IsSelfAndSpouseCoverd()
    {
        $('#hdnIsSelfCovered').val(false);
        $('#hdnIsSpouseCovered').val(false);
        var ChildCount =0;
        $("#divMemberDetails tbody").find("tr").each(function (index) {
            RelationshipWithProspect = $(this).find("[data-name=Relation]").find("option:selected").val();
            if(RelationshipWithProspect =='267'){
                $('#hdnIsSelfCovered').val(true);
            }
            else if(RelationshipWithProspect =='268'){
                $('#hdnIsSpouseCovered').val(true);
            }
            else{
                ChildCount =ChildCount+1;
            }
        })
        $('#hdnNoOfChild').val(ChildCount);

