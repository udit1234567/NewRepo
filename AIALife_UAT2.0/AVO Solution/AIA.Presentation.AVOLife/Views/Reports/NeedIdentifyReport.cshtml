    input[type="range"][multiple] {
        display: block;
        height: 2em;
        margin: 0;
        pointer-events: none;
        margin-top: 14px;
        margin-left: 5px;
    }

        input[type="range"][multiple] + input[type="range"][multiple] {
            margin-top: -2em;
        }

    input[type="range"]::-webkit-slider-thumb {
        pointer-events: all;
    }

    input[type="range"]::-moz-range-thumb {
        pointer-events: all;
    }

    input[type="range"]::-ms-thumb {
        pointer-events: all;
    }

    input[type=range] {
        margin: 18px 0;
        width: 100%;
    }

        input[type=range]:focus {
            outline: none;
        }

        input[type=range]::-webkit-slider-runnable-track {
            width: 100%;
            height: 8.4px;
            cursor: pointer;
            animate: 0.2s;
            background: #b0b2b3;
            border-radius: 1.3px;
        }

        input[type=range]::-webkit-slider-thumb {
            height: 16px;
            width: 5px;
            border-radius: 3px;
            background: #b0b2b3;
            cursor: pointer;
            -webkit-appearance: none;
            margin-top: -14px;
        }

        input[type=range]:focus::-webkit-slider-runnable-track {
            background: #b0b2b3;
        }

        input[type=range]::-moz-range-track {
            width: 100%;
            height: 5.4px;
            cursor: pointer;
            animate: 0.2s;
            background: #b0b2b3;
            border-radius: 1.3px;
        }

        input[type=range]::-moz-range-thumb {
            height: 16px;
            width: 5px;
            border-radius: 3px;
            background: #b0b2b3;
            cursor: pointer;
        }

        input[type=range]::-ms-track {
            width: 100%;
            height: 8.4px;
            cursor: pointer;
            animate: 0.2s;
            background: transparent;
            border-color: transparent;
            border-width: 16px 0;
            color: transparent;
        }

        input[type=range]::-ms-fill-lower {
            background: #b0b2b3;
            border-radius: 2.6px;
        }

        input[type=range]::-ms-fill-upper {
            background: #b0b2b3;
            border-radius: 2.6px;
        }

        input[type=range]::-ms-thumb {
            height: 16px;
            width: 5px;
            border-radius: 3px;
            background: #b0b2b3;
            cursor: pointer;
        }

        input[type=range]::-ms-thumb {
            width: 100%;
            height: 8.4px;
            cursor: pointer;
            animate: 0.2s;
            background: transparent;
            border-color: transparent;
            border-width: 16px 0;
            color: transparent;
        }

        input[type=range]:focus::-ms-fill-lower {
            background: #b0b2b3;
        }

        input[type=range]:focus::-ms-fill-upper {
            background: #b0b2b3;
