@model AIA.Life.Models.UserManagement.ResourceCatagory


<style>
    .embed-responsive {
        position: relative;
        display: block;
        height: 0;
        padding: 0;
        overflow: hidden;
    }
</style>
@foreach (var item in Model.lstResourceCatagorydetails)
{
<div class='embed-responsive pop-style'>
    <br />
    @if(item.ContentType==2879)
    { 
    <a href="@item.Link" width="100%" height="100%" target="_blank">
        <img src="~/@item.imgLink" width="100%" /> 

    </a>
    }
     else
        {
    <a href="~/@item.Link" width="100%" height="100%" target="_blank">
        <img src="~/@item.imgLink" width="100%" />
    </a>
            }
            

     
        @*<embed src="~/Content/Knowledge_Center/1st class Brochure E Final.pdf" width="100%" height="100%" />*@
        <br />
</div>
}
